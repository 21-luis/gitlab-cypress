{
  "name": "gitlab-cypress",
  "version": "1.0.0",
  "description": "Sample project to experiment with Cypress for testing the GitLab application",
  "scripts": {
    "cypress:open": "cypress open",
    "lint": "npx standard",
    "lint:fix": "npx standard --fix",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*'",
    "test:gui": "cypress run --spec cypress/e2e/gui/_index.cy.js"
  },
  "standard": {
    "globals": [
      "before",
      "beforeEach",
      "cy",
      "Cypress",
      "describe",
      "expect",
      "it"
    ]
  },
  "keywords": [
    "testing",
    "automation",
    "cypress",
    "gitlab"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/wlsf82/gitlab-cypress"
  },
  "author": "Walmyr Filho <walmyr@gitlab.com>",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^10.9.0",
    "cypress-plugin-api": "^2.2.1",
    "faker": "^5.5.3",
    "standardjs": "^1.0.0-alpha"
  }
}
